<?xml version='1.0' encoding='utf-8'?>
<plugin id="chinasofti.anyware" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0"
        xmlns:android="http://schemas.android.com/apk/res/android">
    <name>AnywarePlugin</name>
    <js-module name="AnywarePlugin" src="www/AnywarePlugin.js">
        <clobbers target="chinasofti.anyware.AnywarePlugin"/>
    </js-module>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="AnywarePlugin">
                <param name="ios-package" value="AnywarePlugin"/>
            </feature>
        </config-file>
        <source-file src="src/ios/AnywarePlugin.m"/>
    </platform>
</plugin>